{
  "name": "ts-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "NODE_ENV=test; turbo test",
    "start": "NODE_ENV=production; turbo start",
    "docker:build": "docker build -t 'web-push-demo:latest' .",
    "docker:start": "docker run --name web-push-demo -d -p 3000:3000 -p 8888:8888 web-push-demo:latest yarn start"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "prettier": "^3.2.5",
    "turbo": "^2.0.11",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.2.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
